{
	"name": "rdflib-ts",
	"version": "1.2.58",
	"author": "Vladimir Djurdjevic <vladimirdjurdjevic93@gmail.com>",
	"license": "MIT",
	"description": "Typescript implementation of basic RDF model, utils and rdf store with SPARQL 1.1 support.",
	"keywords": [
		"rdf",
		"sparql",
		"triple store"
	],
	"homepage": "https://github.com/vladimirdjurdjevic/rdflib-ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/vladimirdjurdjevic/rdflib-ts.git"
	},
	"bugs": {
		"url": "https://github.com/vladimirdjurdjevic/rdflib-ts/issues",
		"email": "vladimirdjurdjevic93@gmail.com"
	},
	"workspaces": [
		"."
	],
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"clean": "del dist",
		"dev": "tsc -w --preserveWatchOutput",
		"test:unit": "mocha \"test/unit/**/*.spec.ts\"",
		"lint": "eslint \"{test,src}/**/*.ts\"",
		"build": "tsc",
		"rebuild": "yarn run clean && yarn run build",
		"prepublish": "yarn run lint && yarn run test:unit && yarn run rebuild",
		"start:fuseki": "docker run -p 3030:3030 -e ADMIN_PASSWORD=pass123 stain/jena-fuseki:2.5.0",
		"start:tests": "mocha \"test/**/*.spec.ts\"",
		"test": "concurrently -s first -k -n Fuseki,Tests \"yarn run start:fuseki\" \"wait-port 3030 && yarn run start:tests\""
	},
	"dependencies": {
		"jsonld": "0.4.11",
		"n3": "0.10.0",
		"sparql-client-2": "0.6.3",
		"stream-to-string": "1.2.0",
		"superagent": "5.1.0"
	},
	"devDependencies": {
		"@dry-stack/devtools": "1.2.58",
		"@dry-stack/testing": "1.2.57",
		"express": "4.17.1"
	},
	"gitHead": "fcc2ef8615d159a076015bed7083bd254221d78a"
}
